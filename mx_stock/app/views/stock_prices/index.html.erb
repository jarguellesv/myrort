<h1>Listing stock_prices</h1>

<table>
  <tr>
    <th>Price date</th>
    <th>Open</th>
    <th>High</th>
    <th>Low</th>
    <th>Close</th>
    <th>Volume</th>
    <th>Adjusted close</th>
    <th>Stock</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stock_prices.each do |stock_price| %>
  <tr>
    <td><%= stock_price.price_date %></td>
    <td><%= stock_price.open %></td>
    <td><%= stock_price.high %></td>
    <td><%= stock_price.low %></td>
    <td><%= stock_price.close %></td>
    <td><%= stock_price.volume %></td>
    <td><%= stock_price.adjusted_close %></td>
    <td><%= stock_price.stock %></td>
    <td><%= link_to 'Show', stock_price %></td>
    <td><%= link_to 'Edit', edit_stock_price_path(stock_price) %></td>
    <td><%= link_to 'Destroy', stock_price, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Stock price', new_stock_price_path %>
